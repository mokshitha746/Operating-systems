#include <stdio.h>

int main() {
    int frames[10], pages[30], temp[10];
    int i, j, k, l, f, n, pos, faults = 0, max;

    printf("Enter number of frames: ");
    scanf("%d", &f);
    printf("Enter number of pages: ");
    scanf("%d", &n);
    printf("Enter the reference string: ");
    for (i = 0; i < n; i++) scanf("%d", &pages[i]);

    for (i = 0; i < f; i++) frames[i] = -1;

    for (i = 0; i < n; i++) {
        int flag1 = 0, flag2 = 0;
        for (j = 0; j < f; j++) {
            if (frames[j] == pages[i]) { flag1 = flag2 = 1; break; }
        }
        if (flag1 == 0) {
            for (j = 0; j < f; j++) {
                if (frames[j] == -1) {
                    faults++;
                    frames[j] = pages[i];
                    flag2 = 1;
                    break;
                }
            }
        }
        if (flag2 == 0) {
            for (j = 0; j < f; j++) {
                temp[j] = -1;
                for (k = i + 1; k < n; k++) {
                    if (frames[j] == pages[k]) { temp[j] = k; break; }
                }
            }
            pos = 0, max = temp[0];
            for (j = 1; j < f; j++) {
                if (temp[j] == -1) { pos = j; break; }
                if (temp[j] > max) { max = temp[j]; pos = j; }
            }
            frames[pos] = pages[i];
            faults++;
        }
        printf("Frames: ");
        for (l = 0; l < f; l++) printf("%d ", frames[l]);
        printf("\n");
    }
    printf("Total Page Faults = %d\n", faults);
    return 0;
}
